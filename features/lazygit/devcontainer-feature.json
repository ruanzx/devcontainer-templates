    "documentationURL": "https://github.com/jesseduffield/lazygit",
    "licenseURL": "https://github.com/jesseduffield/lazygit/blob/master/LICENSE",
    "keywords": [
        "git",
        "lazygit",
        "tui",
        "terminal-ui",
