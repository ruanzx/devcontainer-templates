{
    "id": "kubectl",
    "version": "1.0.0",
    "name": "kubectl",
    "description": "The Kubernetes command-line tool, kubectl, allows you to run commands against Kubernetes clusters",
    "documentationURL": "https://kubernetes.io/docs/reference/kubectl/",
    "licenseURL": "https://github.com/kubernetes/kubernetes/blob/master/LICENSE",
    "keywords": [
        "kubernetes",
        "k8s",
        "kubectl",
        "cli",
        "cluster-management",
        "devops"
    ],
    "options": {
        "version": {
            "type": "string",
            "description": "Version of kubectl to install",
            "default": "1.31.0"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}
