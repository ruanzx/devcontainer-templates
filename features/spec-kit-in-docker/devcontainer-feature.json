{
  "id": "spec-kit-in-docker",
  "version": "1.0.0",
  "name": "Spec-Kit (Docker-based)",
  "description": "Install spec-kit wrapper that runs in Docker. Build high-quality software faster with executable specifications using AI coding agents.",
  "documentationURL": "https://github.com/github/spec-kit",
  "licenseURL": "https://github.com/github/spec-kit/blob/main/LICENSE",
  "keywords": [
    "spec-kit",
    "spec-driven-development",
    "specifications",
    "ai-development",
    "code-generation",
    "github",
    "docker",
    "container",
    "development-toolkit"
  ],
  "options": {
    "version": {
      "type": "string",
      "proposals": [
        "latest"
      ],
      "default": "latest",
      "description": "Docker image tag for spec-kit. Use 'latest' for the most recent version."
    },
    "imageName": {
      "type": "string",
      "default": "ruanzx/spec-kit",
      "description": "Docker image name for spec-kit."
    }
  },
  "installsAfter": [
    "ghcr.io/devcontainers/features/docker-outside-of-docker:1",
    "ghcr.io/devcontainers/features/docker-in-docker:2"
  ]
}