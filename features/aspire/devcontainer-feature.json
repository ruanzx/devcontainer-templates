{
    "id": "aspire",
    "version": "1.0.3",
    "name": ".NET Aspire",
    "description": ".NET Aspire project templates for building observable, production-ready distributed applications",
    "documentationURL": "https://learn.microsoft.com/en-us/dotnet/aspire/",
    "licenseURL": "https://github.com/dotnet/aspire/blob/main/LICENSE.TXT",
    "keywords": [
        "aspire",
        "dotnet",
        "microservices",
        "distributed-apps",
        "cloud-native",
        "orchestration",
        "observability"
    ],
    "options": {
        "version": {
            "type": "string",
            "description": "Version of .NET Aspire templates to install",
            "default": "9.4.0"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils",
        "ghcr.io/devcontainers/features/dotnet"
    ]
}
