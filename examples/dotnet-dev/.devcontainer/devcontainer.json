{
  "name": ".NET Development Environment",
  "image": "mcr.microsoft.com/dotnet/sdk:10.0",
  "features": {
    "ghcr.io/devcontainers/features/dotnet:latest": {
      "version": "9.0"
    },
    "ghcr.io/ruanzx/features/edit:latest": {
      "version": "1.2.0"
    },
    "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {},
    "ghcr.io/ruanzx/features/spec-kit-in-docker:latest": {
      "version": "latest"
    },
    "ghcr.io/ruanzx/features/markitdown-in-docker:latest": {
      "version": "latest"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "yzhang.markdown-all-in-one",
        "ms-dotnettools.csharp"
      ]
    }
  },
  "postCreateCommand": "./.devcontainer/scripts/postCreateCommand.sh",
  "remoteUser": "root",
  "runArgs": []
}