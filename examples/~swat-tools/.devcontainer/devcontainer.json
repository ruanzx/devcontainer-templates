{
  "name": "SWAT Tools",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu-22.04",
  "features": {
    "ghcr.io/devcontainers/features/common-utils:2": {
      "installZsh": false,
      "configureZshAsDefaultShell": false,
      "installOhMyZsh": false,
      "upgradePackages": false
    },
    "ghcr.io/devcontainers/features/docker-outside-of-docker:1": {},
    "ghcr.io/devcontainers/features/dotnet:latest": {
      "version": "10.0",
      "dotnetRuntimeVersions": "lts",
      "aspnetCoreRuntimeVersions": "lts"
    },
    "ghcr.io/ruanzx/features/spec-kit-in-docker:latest": {},
    "ghcr.io/ruanzx/features/markitdown-in-docker:latest": {},
    "ghcr.io/ruanzx/features/docling-in-docker:latest": {},
    "ghcr.io/ruanzx/features/markify-in-docker:latest": {}
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "yzhang.markdown-all-in-one"
      ]
    }
  },
  "postCreateCommand": "",
  "remoteUser": "root"
}
